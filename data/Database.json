    "common_events": {
        "MAP_1_SHOWINTRO":{
            "commands":[
                "SHOW_TEXT: {'text': 'Vào những năm cuối thời nhà Trần, dưới sự cai trị của Trần An - Trần Thiếu Đế, đất nước ta phải cùng lúc chống thù trong giặc ngoài, dân ta chịu nhiều khốn khổ. Hồ Quý Ly truất ngôi của Trần Thiếu Đế, tự xưng hoàng đế, lấy quốc hiệu là Đại Ngu, lập nên nhà Hồ.'}",
                "SHOW_TEXT: {'text': 'Năm 1406, lấy cớ Phù Trần diệt Hồ, nhà Minh sai Trương Phụ, Mộc Thạnh mang 20 vạn quân sang đánh Đại Ngu. Nhà Hồ bị đánh bại, nước ta rơi vào vòng áp bức của triều Minh.'}",
                "SHOW_TEXT: {'text': 'Trước tình hình đó, mùa xuân năm Mậu Tuất 1418, Lê Lợi đã cùng những hào kiệt, đồng chí hướng như Lê Văn An, Lê Sát, Lê Ngân, Nguyễn Trãi ... tất cả 50 tướng văn và tướng võ, chính thức phất cờ khởi nghĩa.'}",
                "SHOW_TEXT: {'text': 'Trải qua bao nhiêu gian khổ, cuối cùng, Lê Lợi đã khởi nghĩa thành công và chấm dứt chế độ cai trị hơn 20 năm của giặc Minh đồng thời mở ra thời kì độc lập, tự do cho dân tộc.'}"
            ]
        },
        "MAP_6_CUOCCHIEN1": {
            "commands": [
                "SHOW_TEXT: {'text': 'Quân ta ra trận với khí thế sục sôi, giáp trận với quân địch tại Bồ Đằng, Trà Lân khiến Trần Trí, Sơn Thọ hoảng sợ, Lí An, Phương Chính tìm đường thoát thân.'}",
                "SHOW_TEXT: {'text': 'Quân ta chiếm lại được Tây Kinh, Đông Đô. Ninh Kiều máu chảy thành sông, Tốt Động thây chất đầy đồng, Trần Hiệp, Lí Lượng bỏ mạng.'}",
                "SHOW_TEXT: {'text': 'Trước tình thế đó, quân địch không chịu khuất phục, trái lại còn tiếp tục mưu tính, tham công danh. Mã Anh đem quân cứu trận đánh.'}",
                "SWITCHES_ON: {'id': 'MAP_6_TINTUC'}"
            ]
        }
